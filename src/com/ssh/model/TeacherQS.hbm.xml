<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    	<class name="com.ssh.model.TeacherQS" table="TeacherQS">
    		<id name="ID" column="ID">
    		<!-- 配置的是自动增长标识列 -->
    		<generator class="native" /> 
    		</id> 
    		<property name="TeacherID" />
    		<property name="TeacherName" />
    		<property name="EduQuality" />
    		<property name="EduScore" />
    		<property name="ReaQuality" />
    		<property name="ReaScore" />
    		<property name="SerQuality" />
    		<property name="SerScore" />
    		<property name="AssQuality" />
    		<property name="AssScore" />
    		
    	</class>
    </hibernate-mapping>